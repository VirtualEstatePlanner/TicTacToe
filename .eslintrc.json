{
  "extends": ["eslint: recommended", "plugin: import/errors", "prettier"],
  "rules": {
    "no-console": true
  },
  "plugins": ["import", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "root": true
}
